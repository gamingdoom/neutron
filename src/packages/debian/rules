#!/usr/bin/make -f

%:
	dh $@

override_dh_strip_nondeterminism:
	dh_strip_nondeterminism -Xxpi

override_dh_shlibdeps:
	dh_shlibdeps --dpkg-shlibdeps-params="--ignore-missing-info -l${MOZBUILD}/sysroot-${TARGET}/usr/lib/${TARGET} -l${MOZBUILD}/sysroot-${TARGET}/lib -l${MOZBUILD}/sysroot-${TARGET}/lib/${TARGET}"